{
    "version": 2,
    "name": "neahxp-backend",
    "rewrites": [
        {
            "source": "/api/:match*",
            "destination": "/api/index.js"
        },
        {
            "source": "/",
            "destination": "/index.html"
        },
        {
            "source": "/index",
            "destination": "/index.html"
        },
        {
            "source": "/about",
            "destination": "/about.html"
        },
        {
            "source": "/services",
            "destination": "/services.html"
        },
        {
            "source": "/contact",
            "destination": "/contact.html"
        },
        {
            "source": "/portfolio",
            "destination": "/portfolio.html"
        },
        {
            "source": "/blog",
            "destination": "/blog.html"
        },
        {
            "source": "/quote",
            "destination": "/quote.html"
        },
        {
            "source": "/services/:service",
            "destination": "/services/:service.html"
        }
    ],
    "cleanUrls": true
}